# to read complete table 
SELECT * FROM <Table-name>;      # '*" star is for all column

# to read specific header
SELECT <column-name> FROM <table-name>;
SELECT <column1>, <column2>,... FROM <table-name>;

# to get specific data..use WHERE
SELECT * FORM <table-name> WHERE <column-name>=values
SELECT * FORM <table-name> WHERE <column-name>=<column-name>;
